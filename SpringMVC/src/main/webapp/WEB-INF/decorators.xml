		<pattern>/webjars/*</pattern>
		<pattern>/signup</pattern>
		<pattern>/signup/*</pattern>
		<pattern>*/login.jsp*</pattern>
		<pattern>*/lock-screen*.jsp*</pattern>
		<pattern>*/lockScreen.do*</pattern>	
		<pattern>*/keepLockScreenAlive.do*</pattern>
		<pattern>/*showBootStrapMenu.do*</pattern>
